<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.LF2.pro.entity.Month" table="t_month">
		<!--主键 -->
		<id name="id" column="Id" type="integer">
			<generator class="native"></generator>
		</id>
		
		<!--普通属性 -->
		<property name="name" ></property>
		<property name="num1" ></property>
		<property name="num2" ></property>
		<property name="num3" ></property>
		<property name="num4" ></property>
		<property name="num5" ></property>
		<property name="num6" ></property>
		<property name="type" ></property>
		
	</class>
</hibernate-mapping>